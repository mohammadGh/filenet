{
  "name": "filenet",
  "type": "module",
  "version": "0.1.2",
  "packageManager": "pnpm@latest",
  "description": "files on network",
  "author": "mgh <m.ghoreshi@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/mohammadGh/filenet.git",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohammadGh/filenet.git"
  },
  "bugs": "https://github.com/mohammadGh/filenet/issues",
  "keywords": [
    "network",
    "network utility",
    "file server",
    "file transfer"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "filenet": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "unbuild && node dist/cli.mjs",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "cz": "cz",
    "commit": "cz",
    "release": "release-it"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "cli-progress": "^3.12.0",
    "consola": "^3.2.3",
    "read-chunk": "^4.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@types/cli-progress": "^3.11.5",
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.5.0",
    "changelogen": "^0.5.5",
    "commitizen": "^4.3.0",
    "cspell": "^8.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.7",
    "release-it": "^17.2.0",
    "release-it-beautiful-changelog": "^0.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.10",
    "vitest": "^1.5.0"
  },
  "lint-staged": {
    "*.{js,cjs,ts,vue,yml}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
